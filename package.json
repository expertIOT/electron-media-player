    "url": "git+https://github.com/glek/electron-media-player.git"
  },
  "homepage": "https://github.com/glek/electron-media-player#readme",
  "bugs": {
    "url": "https://github.com/glek/electron-media-player/issues"
  },
  "config": {
    "forge": {
      "make_targets": {
