    "@types/jquery": "^2.0.41",
    "@types/lodash": "^4.14.63",
    "angular": "^1.6.4",
    "bootstrap": "^3.3.7",
    "electron-compile": "^6.4.0",
    "fs-extra": "^3.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "tslib": "^1.6.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "electron-prebuilt-compile": "1.6.7-oops.1",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
